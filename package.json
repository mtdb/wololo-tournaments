{
  "name": "wololo-tournaments",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.3.0",
    "@material-ui/icons": "^4.2.1",
    "@reach/router": "^1.2.1",
    "@types/jest": "24.0.15",
    "@types/node": "12.6.8",
    "@types/reach__router": "^1.2.4",
    "@types/react": "16.8.23",
    "@types/react-dom": "16.8.4",
    "@types/react-swipeable-views": "^0.13.0",
    "@types/recharts": "^1.1.20",
    "flag-icon-css": "^3.3.0",
    "moment": "^2.24.0",
    "node-sass": "^4.12.0",
    "portable-fetch": "^3.0.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "3.0.1",
    "react-swipeable-views": "^0.13.3",
    "react-transition-group": "^4.2.2",
    "recharts": "^1.6.2",
    "svg-country-flags": "^1.2.6",
    "tslint": "^5.18.0",
    "typescript": "^3.5.3"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^9.4.4",
    "husky": "^1.3.1",
    "node-fetch": "^2.3.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^6.1.1",
    "postcss-import": "^12.0.1",
    "postcss-nested": "^4.1.1",
    "postcss-preset-env": "^6.5.0",
    "tslint-config-prettier": "^1.17.0",
    "tslint-consistent-codestyle": "^1.15.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-react": "^3.6.0",
    "tslint-react-hooks": "^2.2.1"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run checkAll"
    }
  },
  "scripts": {
    "start": "npm-run-all -p watch:css start-js",
    "build": "npm-run-all build:css build-js",
    "build:css": "postcss src/index.css -o src/__index.css",
    "deploy": "npm-run-all build:css build-js sync-s3",
    "sync-s3": "aws s3 sync build/ s3://wololo-app",
    "watch:css": "postcss src/index.css -o src/__index.css -w",
    "start-js": "react-scripts start",
    "build-js": "react-scripts build",
    "eject": "react-scripts eject",
    "tslint": "tslint --project .",
    "tsc": "tsc",
    "check": "tsc --noEmit",
    "checkAll": "npm run tslint && npm run check"
  }
}
